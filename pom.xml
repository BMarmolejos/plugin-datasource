<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.codenvy.ide</groupId>
        <artifactId>codenvy-ide-parent</artifactId>
        <version>3.0.0-M7-SNAPSHOT</version>
    </parent>
    <artifactId>codenvy-ext-datasource-parent</artifactId>
    <packaging>pom</packaging>
    <name>Codenvy Extension :: Datasource parent</name>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <codenvy.version>3.0.0-M7-SNAPSHOT</codenvy.version>
        <dto-generator-out-directory>${project.build.directory}/generated-sources/dto/</dto-generator-out-directory>
    </properties>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.google.gwt</groupId>
                <artifactId>gwt-user</artifactId>
                <scope>provided</scope>
                <version>${gwt.version}</version>
            </dependency>
            <dependency>
                <groupId>com.google.gwt.inject</groupId>
                <artifactId>gin</artifactId>
                <version>2.1.1</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ide-api</artifactId>
                <version>${codenvy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ide-core</artifactId>
                <version>${codenvy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ext-git</artifactId>
                <version>${codenvy.version}</version>
            </dependency>
            <dependency>
                <groupId>com.codenvy.ide</groupId>
                <artifactId>codenvy-ide-security</artifactId>
                <version>${codenvy.version}</version>
            </dependency>
            <dependency>
                <groupId>junit</groupId>
                <artifactId>junit</artifactId>
                <version>${junit.version}</version>
                <scope>test</scope>
            </dependency>
            <dependency>
                <groupId>net.sourceforge.schemacrawler</groupId>
                <artifactId>schemacrawler</artifactId>
                <version>10.08.01</version>
            </dependency>
            <dependency>
                <groupId>org.postgresql</groupId>
                <artifactId>postgresql</artifactId>
                <version>9.3-1100-jdbc41</version>
            </dependency>
        </dependencies>
    </dependencyManagement>
    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <artifactId>maven-compiler-plugin</artifactId>
                    <version>3.1</version>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>
    <modules>
        <module>codenvy-ext-datasource-core</module>
    </modules>

    <distributionManagement>
        <repository>
            <id>repo.codenvy.private</id>
            <url>https://maven.codenvycorp.com/content/repositories/codenvy-private-releases/</url>
        </repository>
        <snapshotRepository>
            <id>repo.codenvy.private</id>
            <url>https://maven.codenvycorp.com/content/repositories/codenvy-private-snapshots/</url>
        </snapshotRepository>
        <site>
            <id>repo.codenvy.private</id>
            <url>https://maven.codenvycorp.com/content/groups/codenvy-public-group/</url>
        </site>
    </distributionManagement>
</project>
